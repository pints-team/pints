         <apply>
            <eq/>
            <ci>beta_n</ci>
            <apply>
               <times/>
               <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="per_millisecond">0.0000953333</cn>
               <apply>
                  <exp/>
                  <apply>
                     <times/>
